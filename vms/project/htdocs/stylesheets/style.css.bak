img{ border: 0pt; }
body{
margin:0px;
}

#bottom_div{
width: 520px;
height: 150px;
background-color: #b9d1ea;
clear:both;
}

#vms_toolbar {
float:left;
font:normal 11px 'Arial','verdana';
margin:0;
padding:0;
}

#vms_toolbar ul {
background-image:url(../images/bg.gif);
background-repeat:repeat-x;
float:left;
line-height:32px;
list-style:none;
margin:0;
padding:0 10px 0 10px;
/*padding:0 10px 0 10px;
width:500px;*/
}

#vms_toolbar li {
display:inline;
padding:0;
}

#vms_toolbar a {
background: transparent url(../images/divider.gif) no-repeat center right;
color:#000; /* need because it will use default link color */
float:left;
padding:0 3px 0 3px; /* left 3 px because of left.png image */ 
text-decoration:none;

}

#vms_toolbar a span {
display:block;
float:none;
padding:0 10px 0 7px;
}

#vms_toolbar divider{
margin:0
}

#vms_toolbar a span img {
border:none;
margin:8px 4px 0 0;
}

/* show picture when mouse over link */
#vms_toolbar a:hover{
background: url(../images/left-new.gif) no-repeat left center;
}

#vms_toolbar a:hover span {
background:url(../images/right-new.gif) no-repeat right center;
}

#vms_toolbar a.right {
float:right; /* for help button - just add class "right" ->  <a class="right"... */

}



.table {
	border: 1px solid #DDDDDD;
   border-collapse: collapse;
   margin-left:10px;
	empty-cells: show;
	padding:1px;
	}
.table th {
background-image: url(../images/title-light-bg.gif);
height:23px;
font-weight: bold;
text-align:left;
COLOR: #000;
text-transform: capitalize;
padding-left: 5px;
border: 1px solid #D0D0D0;
}

.table td{
		padding: 2px;
		height:20px;
		padding-left:10px;
		padding-right:10px;
		border-bottom: 1px solid #EDEDED;
	}
	
.table th A{
text-decoration: none;
COLOR: #3E4D6C;
}

.table th A:hover{
COLOR: #7E90B3;
text-decoration: underline}

.table th A:visited{
COLOR: #7E90B3;
text-decoration: nine}
